{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard-with-typescript"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/no-unsafe-argument": 0,
        "@typescript-eslint/explicit-function-return-type": 0
    }
}
